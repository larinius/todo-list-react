{
  "printWidth": 140,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": true,
  "arrowParens": "avoid",
  "rangeStart": 0,
  "endOfLine": "auto",
  "plugins": [
    "prettier-plugin-tailwindcss"
  ]
}